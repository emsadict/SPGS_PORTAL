.dud {
  color: Red;
}

.text-copy {
    fill: none;
    stroke: white;
    stroke-dasharray: 6% 29%;
    stroke-width: 5px;
    stroke-dashoffset: 0%;
    animation: stroke-offset 5.5s infinite linear;
}

.text-copy:nth-child(1){
  stroke: red;
  animation-delay: -1;
}

.text-copy:nth-child(2){
  stroke: yellow;
  animation-delay: -2s;
}

.text-copy:nth-child(3){
  stroke: chartreuse;
  animation-delay: -3s;
}

.text-copy:nth-child(4){
  stroke: white;
  animation-delay: -4s;
}

.text-copy:nth-child(5){
  stroke: black;
  animation-delay: -5s;
}

@keyframes stroke-offset{
  100% {stroke-dashoffset: -35%;}
}
</style> 

